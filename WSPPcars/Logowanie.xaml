<Window x:Class="WSPPcars.Logowanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WSPPcars"
        mc:Ignorable="d"
        Title="Logowanie" Height="264" Width="511">
    <Grid Margin="0,0,57,33">
        <Label x:Name="lblNazwa" Content="Nazwa Użytkownika:" HorizontalAlignment="Left" Height="35" Margin="88,71,0,0" VerticalAlignment="Top" Width="152" FontSize="14" FontStyle="Italic"/>
        <Label x:Name="lblHaslo" Content="Hasło:" HorizontalAlignment="Left" Height="35" Margin="88,130,0,0" VerticalAlignment="Top" Width="152" FontSize="14" FontStyle="Italic"/>
        <TextBox x:Name="txtNazwa" HorizontalAlignment="Left" Margin="245,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" Height="35" Background="#FFE0D8D8"/>
        <TextBox x:Name="txtHaslo" HorizontalAlignment="Left" Margin="245,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="163" Height="35" Background="#FFE4E0E0"/>

    </Grid>
</Window>
